<!--section-wrapper-->
<ion-content class="ion-content-body">
  <!---topbar-->
  <facdHeader></facdHeader>

  <!--section-wrapper-->
  <section class="section-wrapper reports">
    <div class="public-title">
      <a class="go-back" (click)="back()"><i class="glyphicon glyphicon-chevron-left"></i></a>
      <h4 class="Title1">护路员信息报送统计表</h4>
      <h4 class="Title2" style="display:none;">护路员绩效考评统计表</h4>
      <a class="btn btn-lg btn-success ShowDiv1" href="#">护路员绩效考评统计表</a>
      <a class="btn btn-lg btn-success ShowDiv2" href="#" style="display:none;">护路员信息报送统计表</a>
    </div>
    <div class="table-wrapper">
      <div id="Div1" class="table-cont">
        <table class="jixoao-tables" cellSpacing="0" cellPadding="0">
          <tr>
            <th>姓名</th>
            <th *ngFor="let item of month; let i = index">{{i+1}} 月</th>
          </tr>
          <tr *ngFor="let item of sendCountData">
            <td>{{item?.userName}}</td>
            <td>{{item?.jan}}</td>
            <td>{{item?.feb}}</td>
            <td>{{item?.mar}}</td>
            <td>{{item?.apr}}</td>
            <td>{{item?.may}}</td>
            <td>{{item?.jun}}</td>
            <td>{{item?.jul}}</td>
            <td>{{item?.aug}}</td>
            <td>{{item?.sep}}</td>
            <td>{{item?.octb}}</td>
            <td>{{item?.nov}}</td>
            <td>{{item?.decb}}</td>

          </tr>

        </table>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
      </div>
      <div id="Div2" class="table-cont" style="display:none;">
