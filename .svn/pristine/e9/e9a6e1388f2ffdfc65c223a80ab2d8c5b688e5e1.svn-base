          <tr></tr>
          <tr>
            <th>&nbsp;</th>
            <th colspan="6" *ngFor="let item of month; let i = index">{{i+1}} 月</th>
          </tr>
          <tr>
            <td>姓名</td>
            <td *ngFor="let item of typeNum; let i=index">{{item}}</td>
          </tr>

          <tr *ngFor="let item of sendScoreData">
            <td>{{item?.userName}}</td>
            <td>{{sum_Distance(item?.monthMap?.month_1)}} KM</td>
            <td>{{sum_send(item?.monthMap?.month_1)}} 分</td>
            <td>{{sum_mark(item?.monthMap?.month_1)}} 分</td>
            <td>{{daily_score(item?.monthMap?.month_1)}} 分</td>
            <td>{{manager_score(item?.monthMap?.month_1)}} 分</td>
            <td style="background-color: aliceblue;">{{sum_score(item?.monthMap?.month_1)}} 分</td>

