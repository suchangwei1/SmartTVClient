@font-face { /* font-light */
  font-family: 'Open Sans-Light';
  src: url('/Fonts/OpenSans-Light.eot');
  src: url('/Fonts/OpenSans-Light.eot?#iefix') format('embedded-opentype'), url('../../Fonts/OpenSans-Light.woff') format('woff'), url('/Fonts/OpenSans-Light.woff2') format('woff2'), url('/Fonts/OpenSans-Light.ttf') format('truetype'), url('/Fonts/OpenSans-Light.svg#OpenSans-Regular') format('svg');
}

@font-face { /* font-regular */
  font-family: 'Open Sans-Regular';
  src: url('/Fonts/OpenSans-Regular.eot');
  src: url('/Fonts/OpenSans-Regular.eot?#iefix') format('embedded-opentype'), url('../../Fonts/OpenSans-Regular.woff') format('woff'), url('/Fonts/OpenSans-Regular.woff2') format('woff2'), url('/Fonts/OpenSans-Regular.ttf') format('truetype'), url('/Fonts/OpenSans-Regular.svg#OpenSans-Regular') format('svg');
}

@font-face { /* font-bold */
  font-family: 'Open Sans-Bold';
  src: url('/Fonts/OpenSans-Bold.eot');
  src: url('/Fonts/OpenSans-Bold.eot?#iefix') format('embedded-opentype'), url('../../Fonts/OpenSans-Bold.woff') format('woff'), url('/Fonts/OpenSans-Bold.woff2') format('woff2'), url('/Fonts/OpenSans-Bold.ttf') format('truetype'), url('/Fonts/OpenSans-Bold.svg#OpenSans-Regular') format('svg');
}
.infinite-loading-text{
  color: wheat;
}
.vjs-default-skin .vjs-big-play-button {left:40%;top:40%;}

.ion-content-body{
  font-family: "Microsoft YaHei", "Hiragino Sans GB", "微软雅黑",tahoma,arial,simsun,"宋体";
  font-size: 14px;
  line-height: normal;
  font-weight: 300;
  background: url(../assets/content/images/bg.jpg) bottom center no-repeat;
  background-size: cover;
}
.overflow-hidden{ overflow: hidden; }
a, u{ text-decoration: none; }

.container{ width: 1200px !important; position: relative; }

.align-center{ text-align: center; }
.align-right{ text-align: right; }
.align-left{ text-align: left; }

.clore-white{ color: #fff;}

.margin-0px{ margin: 0;}

.white-160px{ width: 160px; }

/*******************************start*******************************/
.start-section{
  position: absolute;
  width: 50%;
  height: auto;
  left: 50%;
  top: 20%;
  margin-left: -25%;
  text-align: center;
}
.start-section img{
  border: 0;
  border-radius: 50%;
  -moz-box-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  -webkit-box-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  box-shadow:1px 3px 12px rgba(0, 0, 0, .35);
}
.start-section h1{
  text-align: center;
  color: #fff;
  -moz-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  -webkit-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
}

/*******************************top & footer*******************************/
.topbar{
  display: block;
  float: left;
  width: 100%;
  padding: 30px 0;
  box-sizing: content-box;
}
.topbar .logo{
  float: left;
  width: 80px;
  border: 0;
  border-radius: 50%;
  -moz-box-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  -webkit-box-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  box-shadow:1px 3px 12px rgba(0, 0, 0, .35);
}
.topbar h2{
  display: inline-block;
  color: #fff;
  padding: 0 15px;
  margin: 0;
  line-height: 80px;
  -moz-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  -webkit-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
}
.topbar h3{
  color: #fff;
  text-align: right;
  margin: 0;
  line-height: 80px;
  -moz-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  -webkit-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
}
.marquee-box{
  float: left;
  position: relative;
  width: 100%;
  height: 48px;
  margin-bottom: 30px;
  box-sizing: content-box;
}
.marquee-box span{
  float: left;
  position: absolute;
  width: 30px;
  height: 40px;
  left: 110px;
  background: url(../assets/content/images/icon-01.png) left center no-repeat;
  background-size:100%;
}
.marquee-box marquee{
  margin: 0 170px;
}
.marquee-box marquee h6{
  display : inline;
  margin: 0;
  padding-left: 18px;
  height: 48px;
  line-height: 48px;
  background: url(../assets/content/images/icon-05.png) left center no-repeat;
  background-size: 4px;
}
.marquee-box marquee a{
  text-decoration: none;
  color: #fff;
  -moz-text-shadow:0px 1px 5px rgba(0, 0, 0, .35);
  -webkit-text-shadow:0px 1px 5px rgba(0, 0, 0, .35);
  text-shadow:0px 1px 5px rgba(0, 0, 0, .35);
  line-height: 22px;

}
footer{
  display: block;
  position: relative;
  box-sizing: content-box;
  text-align: center;
}
footer h5{
  margin: 0;
  line-height: 80px;
  color: #fff;
  -moz-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  -webkit-text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
  text-shadow:1px 3px 12px rgba(0, 0, 0, .35);
}

/*******************************top & header*******************************/
.section-wrapper{
  display: block;
  box-sizing: content-box;
  position: relative;
}
.section-wrapper.index{
  display: -webkit-box; /* OLD - iOS 6-, Safari 3.1-6 */
  display: -moz-box; /* OLD - Firefox 19- (buggy but mostly works) */
  display: -ms-flexbox; /* TWEENER - IE 10 */
  display: -webkit-flex; /* NEW - Chrome */
  display: flex; /* NEW, Spec - Opera 12.1, Firefox 20+ */
  box-sizing: content-box;
  position: relative;
}
.section-wrapper.index .items{
  position: relative;
  display: block;
  box-sizing: content-box;
  -webkit-box-flex: 1; /* OLD - iOS 6-, Safari 3.1-6 */
  -moz-box-flex: 1; /* OLD - Firefox 19- */
  -webkit-flex: 1; /* Chrome */
  -ms-flex: 1; /* IE 10 */
  flex: 1;
  margin: 0 30px;
    border-radius: 12px;
  overflow: hidden;
}
.section-wrapper.index .items:hover{
  /*-moz-box-shadow:1px 5px 25px rgba(10, 60, 90, .85);
  -webkit-box-shadow:1px 5px 25px rgba(10, 60, 90, .85);
  box-shadow:1px 5px 25px rgba(10, 60, 90, .85);*/
  -moz-box-shadow:1px 5px 25px rgba(0, 0, 255, 1);
  -webkit-box-shadow:1px 5px 25px rgba(0, 0, 255, 1);
  box-shadow:1px 5px 25px rgba(0, 0, 255, 1);
  opacity: 1;
}
.section-wrapper.index .active{
  -moz-box-shadow:1px 5px 25px rgba(255, 0, 0, 1);
  -webkit-box-shadow:1px 5px 25px rgba(255, 0, 0, 1);
  box-shadow:1px 5px 25px rgba(255, 0, 0, 1);
}

.section-wrapper.index .items.bg-01{
  background: url(../assets/content/images/bg-img-01.jpg) top center no-repeat;
  background-size: 100%;
}
.section-wrapper.index .items.bg-02{
  background: url(../assets/content/images/bg-img-02.jpg) top center no-repeat;
  background-size: cover;
}
.section-wrapper.index .items.bg-03{
  background: url(../assets/content/images/bg-img-03.jpg) top center no-repeat;
  background-size: 100%;
}
.section-wrapper.index .items.bg-04{
  background: url(../assets/content/images/bg-img-04.jpg) top center no-repeat;
  background-size: cover;
}
.section-wrapper.index .items h2{
  position: absolute;
  left: 0;
  bottom: 0;
  padding: .75em 1em;
  margin: 0;
  width: 100%;
  text-align: center;
  color: #fff;
  background: rgba(0, 0, 0, .5);
    border-radius: 0 0 12px 12px;
}
.section-wrapper.index .items h2:hover{
  font-weight: 600;
}

/*******************************layout*******************************/
body .layui-layer.layout-news-domain{ border-radius: 14px; overflow: hidden; }
body .layout-news-domain .layui-layer-content{ overflow: hidden; padding: 15px;}
body .layui-layer.layout-news-domain .layui-layer-close{
    right: 0;
    top: 0;
    width: 48px;
    height: 48px;
    margin-top: -12px;
    margin-right: -12px;
    background: rgba(0, 0, 0, .5) url(../assets/content/images/icon-03.png) center center no-repeat;
    background-size: 60%;
    border-radius: 0 0 0 14px;
}
body .layui-layer.layout-news-domain .layui-layer-close:hover{
    opacity: .75;
}
.layout-wrapper{
  display: block;
  position: relative;
  box-sizing: content-box;
  //display: none;
}
.layout-wrapper2{
  display: block;
  position: relative;
  box-sizing: content-box;
  //display: none;
}
.layout-container{
  display: block;
  box-sizing: content-box;
  position: relative;
  padding: 15px;
}

/*******************************news*******************************/
.news-container{
  display: block;
  box-sizing: content-box;
  position: relative;
}
.news-container .title{
  display: block;
  position: relative;
  box-sizing: content-box;
  height: 60px;
}
.news-container .title strong{
  float: left;
  box-sizing: content-box;
  width: 90px;
  height: 60px;
  padding-left: 20px;
  margin-left: -30px;
  background: url(../assets/content/images/icon-02.png) right center no-repeat;
  font-size: 30px;
  line-height: 56px;
  color: #fff;
}
.news-container .title h3{
  display: block;
  margin: 0;
  padding: 0 15px;
  line-height: 58px;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.news-container .content{
  display: block;
  position: relative;
  box-sizing: content-box;
  height: 534px;
  margin-top: 20px;
  font-size: 18px;
  line-height: 36px;
  color: #333;
  overflow: hidden;
  overflow-y: auto;
}
.news-container .content::-webkit-scrollbar-track
{
  border-radius: 8px;
  background: #ddd;
}

.news-container .content::-webkit-scrollbar
{
  width: 8px;
  background: #F5F5F5;
}

.news-container .content::-webkit-scrollbar-thumb
{
  border-radius: 8px;
  background-color: #999;
}

/*******************************maps*******************************/
.section-wrapper.maps{
  box-sizing: content-box;
  margin: 0 30px;
  border-radius: 14px;
  overflow: hidden;
}
.public-title{
  position: absolute;
  box-sizing: border-box;
  width: 100%;
  height: 64px;
  padding: 0 20px;
  z-index: 2;
}
.public-title .go-back{
  float: left;
  width: 84px;
  height: 44px;
  margin: 10px 0;
  border-radius: 8px;
  background: rgba(0, 0, 0, .5);
  font-size: 24px;
  text-align: center;
  color: #fff;
  line-height: 44px;
}
.public-title .go-back:hover{
  background: rgba(0, 0, 0, .75);
}
.public-title .go-home{
  float: right;
  width: 84px;
  height: 44px;
  margin: 10px 0;
  border-radius: 8px;
  background: rgba(0, 0, 0, .5);
  font-size: 20px;
  text-align: center;
  color: #fff;
  line-height: 44px;
  /*border-left: 1px solid rgba(255, 255, 255, 0.75);*/
}
.public-title .btn{
  float: right;
  padding: 9px 15px;
