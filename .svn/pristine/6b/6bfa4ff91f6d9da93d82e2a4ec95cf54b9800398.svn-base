  padding: 15px;
  padding-right: 10px;
  border-radius: 0 0 8px 8px;
  overflow: hidden;
}
.message-allbox .listview{
  display: block;
  box-sizing: content-box;
  position: relative;
  padding-right: 5px;
  overflow: hidden;
  overflow-y: auto;
}
.message-allbox .listview::-webkit-scrollbar-track
{
  /*-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);*/
  border-radius: 6px;
  background: #ddd;
}
.message-allbox .listview::-webkit-scrollbar
{
  width: 6px;
  /*background: #F5F5F5;*/
}
.message-allbox .listview::-webkit-scrollbar-thumb
{
  border-radius: 6px;
  background-color: #999;
}
.message-allbox .listview{
  display: block;
  box-sizing: content-box;
  position: relative;
}
.message-allbox .listview .listgroups{
  float: left;
  position: relative;
  padding-left: 20px;
  clear: both;
}
.message-allbox .listview .listgroups .border{
  position: absolute;
  left: 0;
  top: 0;
  width: 14px;
  height: 100%;
}
.message-allbox .listview .listgroups .border i{
  position: absolute;
  margin: 21px 0;
  width: 14px;
  height: 14px;
  background: #389818;
  border-radius: 50%;
  z-index: 2;
  overflow: hidden;
}
.message-allbox .listview .listgroups .border u{
  position: absolute;
  width: 6px;
  height: 100%;
  margin: 0 4px;
  background: #389818;
  overflow: hidden;
}
.message-allbox .listview .listgroups:first-child .border u{
  border-radius: 3px 3px 0 0;
}
.message-allbox .listview .listgroups:last-child .border u{
  border-radius: 0 0 3px 3px;
}
.message-allbox .listview .listgroups .content{
  display: block;
  box-sizing: content-box;
}
.message-allbox .listview .listgroups .content h6{
  display: inline-block;
  clear: both;
  margin: 15px 0;
  padding: 0 15px;
  line-height: 28px;
  background: #fff;
  border: 1px solid #aaa;
  border-radius: 15px;
}
.message-allbox .listview .listgroups .content .mess-inbox{
  margin: 0;
  float: left;
  width: 100%;
}
.message-allbox .listview .listgroups .content .mess-inbox .mess-items{
  float: left;
  width: 270px;
  margin: 7.5px;
}

.section-wrapper.media.pages{
  position: relative;
  margin: 30px;
  border-radius: 8px;
  overflow: hidden;
}
.section-wrapper.return{
  margin: 0 30px;
  background: rgba(255, 255, 255, .75);
  border-radius: 8px;
}
.videos-return{
  clear: both;
  display: block;
  box-sizing: border-box;
  position: relative;
  margin: 0 30px;
  top: 100px;
  overflow: hidden;
}
.section-wrapper.reports{
  margin:30px;
  margin-top: 0;
  border-radius: 8px;
  background: rgba(255, 255, 255, .75);
}
.section-wrapper.reports .table-wrapper{
  display: block;
  box-sizing: border-box;
  position: relative;
  margin: 0 20px;
  top: 80px;
  overflow: auto;
}

.section-wrapper.reports .table-wrapper::-webkit-scrollbar-track
{
  /*-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);*/
  border-radius: 6px;
  background: #ddd;
}
.section-wrapper.reports .table-wrapper::-webkit-scrollbar
{
  width: 6px;
  height: 6px;
  /*background: #F5F5F5;*/
}
.section-wrapper.reports .table-wrapper::-webkit-scrollbar-thumb
{
  border-radius: 6px;
  background-color: #999;
}
.table-cont{
  display: block;
  box-sizing: content-box;
  position: relative;
}
.jixoao-tables{
  display: table;
  width: 100%;
  background: #fff;
}
.jixoao-tables tr th, .jixoao-tables tr td{
  padding: 10px;
  text-align: center;
  min-width: 80px;
}
.jixoao-tables tr th{
  border-top: 1px solid #ddd;
  border-bottom: 5px solid #ddd;
  border-left: 1px solid #ddd;
  background: #f5f5f5;
}
.jixoao-tables tr th:first-child{
  border-left: 0;
}
.jixoao-tables tr td{
  border-bottom: 1px solid #ddd;
  border-left: 1px solid #ddd;
}
.jixoao-tables tr td:first-child{
  border-left: 0;
}


