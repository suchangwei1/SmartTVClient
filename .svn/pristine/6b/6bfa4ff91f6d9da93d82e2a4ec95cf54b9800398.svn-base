}
.carousel-control.left span, .carousel-control.right span{
  //margin-top: -110px;
}
.videos-wrapper {
  display: inline-block;
  box-sizing: content-box;
  position: relative;
  margin: 0 auto;
  overflow: hidden;
}
.videos-wrapper video {
  display: inline-block;
  box-sizing: content-box;
  //height: 100%;
  height: 350px;
  width: 500px;
  margin: auto;
