  text-align: center;
  color: #fff;
  background: rgba(0, 0, 0, .5);
  border-radius: 0 0 8px 8px;
}
.maps-wrapper{
  display: block;
  box-sizing: content-box;
  position: absolute;
  width: 100%;
  height: 100%;
  //background: #fff url(../assets/content/images/maps.png) center center no-repeat;
