  overflow-y: auto;
}
.user-group::-webkit-scrollbar-track
{
  /*-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);*/
  border-radius: 6px;
  background: #ddd;
}
.user-group::-webkit-scrollbar
{
  width: 6px;
  /*background: #F5F5F5;*/
}
.user-group::-webkit-scrollbar-thumb
{
  border-radius: 6px;
  background-color: #999;
}
.user-group img{
  max-width: 50%;
}
.user-group p{
  margin: 0;
  font-size: 15px;
  line-height: 30px;
}
.user-group .advance{
  margin: 30px 0;
