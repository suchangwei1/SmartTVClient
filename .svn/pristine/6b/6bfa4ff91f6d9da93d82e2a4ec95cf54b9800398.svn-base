//隐藏video全屏按钮
video::-webkit-media-controls-fullscreen-button {display: none;}

.section-wrapper.video{
  display: block;
  box-sizing: content-box;
  position: relative;
  margin: 0 30px;
  border-radius: 8px;
  overflow: hidden;
}
.section-wrapper.video img{
